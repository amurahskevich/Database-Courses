@model Contracts.Category.CategoryDto

<form asp-action="Create" method="post">
    <div class="form-group">
        <label for="exampleInputEmail1">Идентификационный номер</label>
        <input asp-for="Id" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" readonly>
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Название категории</label>
        <input asp-for="Name" type="text" class="form-control" id="exampleInputPassword1">
    </div>
    <button type="submit" class="btn btn-primary">Сохранить</button>
</form>


